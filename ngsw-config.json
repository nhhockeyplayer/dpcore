{
  "index": "/index.html",
  "dataGroups": [
    {
      "name": "api-freshness",
      "urls": ["https://api.chucknorris.io/jokes/random"],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 20,
        "maxAge": "3d",
        "timeout": "5s"
      }
    }
  ],
  "routes": {
    "/": {
      "prefix": false
    },
    "/con": {
      "prefix": true
    },
    "/content": {
      "prefix": false
    },
    "/contentprojector": {
      "prefix": false
    },
    "/contentprojection": {
      "prefix": false
    }
  },
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.scss",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ],
        "urls": [
          "https://fonts.googleapis.com/**"
        ]
      }
    }
  ]
}
